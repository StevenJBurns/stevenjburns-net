@{var title = PageData["title"];}
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <title>@title</title>
    <link type="text/css" rel="stylesheet" href="@Href("/css/page.css")" />
    <link type="image/png" rel="icon" href="@Href("/content/icons/favicon.png")" />
    <script type="text/javascript">
      var _gaq = _gaq || [];
      _gaq.push(['_setAccount', 'UA-35847828-1']);
      _gaq.push(['_trackPageview']);
      
      (function(){
        var ga = document.createElement('script');
        ga.type = 'text/javascript';
        ga.async = true;
        ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
        
        var s = document.getElementsByTagName('script')[0];
        s.parentNode.insertBefore(ga, s);
      })();
    </script>
  </head>
  <body>
    <header>
      <div class="table" style="margin: 0; padding: 0; background: transparent; border-radius: 0">
        <div class="cell">
          <a href="../"><img src="../content/images/bullseye/bullseye.lightgray.png" alt="Home" style="height: 48px; width: 48px; display: block; border-style: none" /></a>
        </div>
        <div class="cell">
          <h3 style="line-height: 48px">Digital ramblings, musings and whimsy</h3>
        </div>
      </div>
    </header>
@RenderBody()
    <footer></footer>
  </body>
</html>
